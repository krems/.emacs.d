def qsort(a):
#    if len(a) < 7:
#        insertionSort(a)
    pivotPos = random(0, len(a))
    pivot = a[pivotPos]
    left = list()
    right = list()
    for x in a:
        if x < pivot:
            left.add(x)
        else:
            right.add(x)
    a = left + right
    qsort(a[0:pivotPos])
    qsort(a[pivotPos:])
