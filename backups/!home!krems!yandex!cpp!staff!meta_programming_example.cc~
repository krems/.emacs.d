#include <iostream>

template<int A, int N>
struct Power {
  static const int Result = N > 0 ? A * Power<A, N-1>::Result : 1;
};

template<int A>
struct Power<A, 0> {
  static const int Result  = 1;
};

template<int A, int B>
struct Divides {
  static const bool Result = B % A == 0;
};

/*
template<int M>
class Dummy;
*/

int main() {
  Dummy< Power<2, 10>::Result>();
  // std::cout << Power<2, 10>::Result << std::endl;
}
