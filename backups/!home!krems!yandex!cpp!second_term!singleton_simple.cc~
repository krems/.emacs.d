#include <iostream>

class Singleton {
  Singleton() {}
  Singleton(const Singleton&) {}
  Singleton& operator =(const Singleton&);
  static Singleton* instance = NULL;
 public:
  static Singleton* getInstance() {
    if (instance == NULL) {
      instance = new Singleton;
    }
    return instance;
  }
};

int main() {
  return 0;
}
