#include <iostream>
#include <string>

using std::string;
using std::cout;
using std::endl;


int getProblremNumber(const string& text, int numberOfProblems = 3) {
  int result = 0;
  for (int i = 0; i < text.length(); ++i) {
    result = result * 239 + text[i];
  }
  return (result % numberOfProblems + numberOfProblems) % numberOfProblems + 1;
}

int main() {
  cout << getProblemNumber("") << endl;
  return 0;
}
