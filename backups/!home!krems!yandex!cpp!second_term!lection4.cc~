#include <iostream>
#include <allocator>
#include <vector>

template<typename T>
class Allocator : public std:allocator<T> {
 public:
  Allocator() throw() {
    std::cout << "ctor" << std::endl;
  }
  ~Allocator() throw() {
    std::cout << "dtor" <<std::endl;
  }
};


int main() {
  std::vector<int, Allocator<int> > v;
  v.push_back(0);
  return 0;
}
