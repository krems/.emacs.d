
using namespace std;
struct Num {
  int value;
  Num operator ++ (int) {
    Num copy(*this);
    value++;
    return copy;
  }
  Num& operator ++ () {
    ++value;
    return *this;
  }
};
int main() {
  Num i;
  i.value = 1;
  ++++i;
  cout << i.value << " ";
  i.value = 1;
  i++++;
  cout << i.value << endl;
}
