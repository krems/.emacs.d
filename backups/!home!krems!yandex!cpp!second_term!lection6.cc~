#include <iostream>
#include <vector>
#include <utility>

using std::cout;
using std::endl;
using std::vector;

class Class {
 public:
  Class() {}
  Class(Class&& tmp) {}
  
};

int main() {
  vector<int> v {0, 1, 2};
  int e {2};
  // vector<int> v = {0, 1, 2};
  // vector<int> v({0, 1, 2});              
  for (int& x: v) {
    cout << ++x << endl;
  }
  for (auto i = v.cbegin(); i != v.cend(); ++i) {
    cout << *i << endl;
  }
  Class a(Class());
  Class b;
  Class c(std::move(b));
  return 0;
}
